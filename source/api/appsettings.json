{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    }
  },

  "AllowedHosts": "*",

  "Authentication": {
    "ApiKey": ""
  },

  "RateLimiting": {
    "Authenticated": {
      "PermitLimit": 30,
      "WindowSeconds": 60,
      "QueueLimit": 0
    },
    "Anonymous": {
      "PermitLimit": 10,
      "WindowSeconds": 60,
      "QueueLimit": 0
    }
  },

  "QueryPerformance": {
    "SlowQueryThresholdMilliseconds": 250
  }
}